{
  "expo": {
    "name": "Finance Track",
    "slug": "finance-track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "financetrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { "supportsTablet": true },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.godwingbewezoun.financetrack"
    },
    "web": { "output": "static", "favicon": "./src/assets/images/favicon.png" },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": { "backgroundColor": "#000000" }
        }
      ]
    ],
    "experiments": { "typedRoutes": true, "reactCompiler": true },
    "extra": { "router": {}, "eas": { "projectId": "7f38d94d-874d-423a-8df8-42b9b06a3f71" } }
  }
}
